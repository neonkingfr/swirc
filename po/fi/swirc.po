#: ../src/main.cpp:120
msgid ""
"\n"
"Options:\n"
"\n"
msgstr ""
"\n"
"Vaihtoehdot:\n"
"\n"

#: ../src/main.cpp:121
msgid "    -4                   Use IPv4 addresses only\n"
msgstr "    -4                   Käytä IPv4 osoitteita vain\n"

#: ../src/main.cpp:122
msgid "    -6                   Use IPv6 addresses only\n"
msgstr "    -6                   Käytä IPv6 osoitteita vain\n"

#: ../src/main.cpp:123
msgid "    -?, --help           Output help\n"
msgstr "    -?, --help           Lähtö help\n"

#: ../src/main.cpp:124
msgid "    -C                   Do not change color definitions\n"
msgstr "    -C                   Älä muuta värimääritysten\n"

#: ../src/main.cpp:125
msgid "    -P                   Permanently disable SASL authentication\n"
msgstr "    -P                   Poista SASL todennus pysyvästi käytöstä\n"

#: ../src/main.cpp:126
msgid "    -R                   Disable TLS/SSL peer verification\n"
msgstr "    -R                   Poista käytöstä TLS/SSL vertaisvahvistus\n"

#: ../src/main.cpp:131
msgid "    -c <server[:port]>   Connect to IRC server\n"
msgstr "    -c <server[:port]>   Muodosta yhteys IRC palvelimeen\n"

#: ../src/main.cpp:132
msgid "    -d                   Debug logging\n"
msgstr "    -d                   Kirjaa virheenkorjausviestejä\n"

#: ../src/main.cpp:133
msgid "    -i                   Turn on Internet Citizen's Band mode\n"
msgstr "    -i                   Ota Internet Citizen's Band tila käyttöön\n"

#: ../src/main.cpp:134
msgid "    -j <join>            A comma-separated list of channels to join\n"
msgstr "    -j <join>            Pilkuilla erotettu luettelo kanavista\n"

#: ../src/main.cpp:135
msgid "    -n <nickname>        Online nickname\n"
msgstr "    -n <nickname>        Online nickname\n"

#: ../src/main.cpp:136
msgid ""
"    -p                   Query for server password (for private servers)\n"
msgstr ""
"    -p                   Kyselyn palvelimen salasanaa (yksityisten "
"palvelimien)\n"

#: ../src/main.cpp:137
msgid "    -r <rl name>         Your real name\n"
msgstr "    -r <rl name>         Oikea nimesi\n"

#: ../src/main.cpp:138
msgid "    -u <username>        Your username\n"
msgstr "    -u <username>        Sinun käyttäjä nimesi\n"

#: ../src/main.cpp:139
msgid "    -v, --version        Output Swirc version\n"
msgstr "    -v, --version        Lähtö Swirc versio\n"

#: ../src/main.cpp:140
msgid "    -x <config>          Config file\n"
msgstr "    -x <config>          Config tiedosto\n"

#: ../src/sig-w32.c:26
msgid "Abnormal termination"
msgstr "Keskeytetty"

#: ../src/sig-w32.c:27
msgid "Floating-point error"
msgstr "Liukulukupoikkeus"

#: ../src/sig-w32.c:28
msgid "Illegal instruction"
msgstr "Virheellinen käsky"

#: ../src/sig-w32.c:29
msgid "Illegal storage access"
msgstr "Muistialueen ylitys"

#: ../src/sig-w32.c:30
msgid "Termination request"
msgstr "Päätetty"

#: ../src/main.cpp:201
#, c-format
msgid "Usage: %s [OPTION] ...\n"
msgstr "Käyttö: %s [VAIHTOEHTO] ...\n"
