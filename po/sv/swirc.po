#: ../src/main.cpp:120
msgid ""
"\n"
"Options:\n"
"\n"
msgstr ""
"\n"
"Flaggor:\n"
"\n"

#: ../src/main.cpp:121
msgid "    -4                   Use IPv4 addresses only\n"
msgstr "    -4                   Använd enbart IPv4 adresser\n"

#: ../src/main.cpp:122
msgid "    -6                   Use IPv6 addresses only\n"
msgstr "    -6                   Använd enbart IPv6 adresser\n"

#: ../src/main.cpp:123
msgid "    -?, --help           Output help\n"
msgstr "    -?, --help           Skriv ut hjälp\n"

#: ../src/main.cpp:124
msgid "    -C                   Do not change color definitions\n"
msgstr "    -C                   Ändra inte färgdefinitioner\n"

#: ../src/main.cpp:125
msgid "    -P                   Permanently disable SASL authentication\n"
msgstr "    -P                   Stäng av SASL autentisering permanent\n"

#: ../src/main.cpp:126
msgid "    -R                   Disable TLS/SSL peer verification\n"
msgstr "    -R                   Stäng av TLS/SSL peer verifikation\n"

#: ../src/main.cpp:131
msgid "    -c <server[:port]>   Connect to IRC server\n"
msgstr "    -c <server[:port]>   Anslut till IRC server\n"

#: ../src/main.cpp:132
msgid "    -d                   Debug logging\n"
msgstr "    -d                   Debug loggning\n"

#: ../src/main.cpp:133
msgid "    -i                   Turn on Internet Citizen's Band mode\n"
msgstr "    -i                   Sätt på Internet Citizen's Band läge\n"

#: ../src/main.cpp:134
msgid "    -j <join>            A comma-separated list of channels to join\n"
msgstr ""
"    -j <join>            En kommaseparerad lista bestående av kanaler att "
"öppna\n"

#: ../src/main.cpp:135
msgid "    -n <nickname>        Online nickname\n"
msgstr "    -n <nickname>        Online nickname\n"

#: ../src/main.cpp:136
msgid ""
"    -p                   Query for server password (for private servers)\n"
msgstr ""
"    -p                   Fråga efter serverlösenord (för privata servrar)\n"

#: ../src/main.cpp:137
msgid "    -r <rl name>         Your real name\n"
msgstr "    -r <rl name>         Ditt riktiga namn\n"

#: ../src/main.cpp:138
msgid "    -u <username>        Your username\n"
msgstr "    -u <username>        Ditt användarnamn\n"

#: ../src/main.cpp:139
msgid "    -v, --version        Output Swirc version\n"
msgstr "    -v, --version        Visa Swirc version\n"

#: ../src/main.cpp:140
msgid "    -x <config>          Config file\n"
msgstr "    -x <config>          Konfigurationsfil\n"

